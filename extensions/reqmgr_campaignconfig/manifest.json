{
    "manifest_version": 3,
    "name": "Open Campaign Config",
    "version": "1.0",
    "description": "Open Open Campaign Config in a new tab with the selected text",
    "permissions": [
        "contextMenus",
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon.png"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}