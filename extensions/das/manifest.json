{
    "manifest_version": 3,
    "name": "DAS Page Opener",
    "version": "1.0",
    "description": "Open DAS Page with selected text as a query parameter.",
    "permissions": ["contextMenus", "activeTab", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon.png"
    },
    "host_permissions": ["http://*/*", "https://*/*" ]
}
