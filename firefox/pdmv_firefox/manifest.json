{
    "manifest_version": 2,
    "name": "Open pMp",
    "version": "1.0",
    "description": "Open pMp Page with selected text as a query parameter.",
    "permissions": [
        "contextMenus",
        "activeTab",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "48": "icon.png"
    }
}